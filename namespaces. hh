// std
using std::cout;
using std::endl;
using std::string;
using std::wstring;
using std::vector;
using std::stringstream;
using std::of stream;
// new:json
namespace json {
  //Vector to json
  string vectorToJson(const vector<string>& vec) {
    stringstream ss;
    ss << "[";
    for (size_t i = 0; i < vec.size(); ++i) {
        ss << "\"" << vec[i] << "\"";
        if (i < vec.size() - 1) {
            ss << ",";
        }
    }
    ss << "]";
    return ss.str();
}

}

// new:maths
